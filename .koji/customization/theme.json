{
  "theme": {
    "--font-family": "Nunito",
    "--text-color": "#0F0F0F",
    "--navbar-background-color": "#fff",
    "--footer-background-color": "#FFF",
    "--background-image": "https://images.koji-cdn.com/d2a7a9d4-7373-4d11-8310-92a8d0394128/backgroundnew.jpg",
    "home_main_image": "https://images.koji-cdn.com/58994a80-f750-44f6-8e3c-454a769d4c1b/angular8typescriptscaffoldlogo.png"
  },
  "@@editor": [
    {
      "key": "theme",
      "name": "Theme settings",
      "icon": "ðŸ’…",
      "source": "theme.json",
      "fields": [
        {
          "key": "--font-family",
          "name": "Font Family",
          "description": "Google font NAME to use on page"
        },
        {
          "key": "home_main_image",
          "name": "Home main Image",
          "description": "URL of Image to place",
          "type": "image"
        },
        {
          "key": "--background-image",
          "name": "Background Image",
          "description": "URL of Image to place as background",
          "type": "image"
        },
        {
          "key": "--text-color",
          "name": "Text Color",
          "description": "Color for text content (not links)",
          "type": "color"
        },
        {
          "key": "--navbar-background-color",
          "name": "Navbar background color",
          "description": "Navbar background color",
          "type": "color"
        },
        {
          "key": "--footer-background-color",
          "name": "Footer background color",
          "description": "Footer background color",
          "type": "color"
        }
      ]
    }
  ]
}